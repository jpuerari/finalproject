  "bugs": {
    "url": "https://github.com/jpuerari/finalproject/issues"
  },
  "homepage": "https://github.com/jpuerari/finalproject#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^4.0.1",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "react-dev-utils": "^10.2.1"
  }
